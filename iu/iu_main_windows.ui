   <string notr="true">/* Estilo para el fondo del QWidget en modo oscuro */
QWidget {
    background-color: #121212; /* Fondo oscuro */
    color: #E8EAF6; /* Texto claro en el fondo oscuro */
}

/* Estilo para la barra de menús */
QMenuBar {
    background-color: #333333; /* Fondo oscuro de la barra */
    color: #E8EAF6; /* Texto claro en el menú */
    font-size: 14px;
    font-weight: 600;
}

/* Elementos de los menús en la barra */
QMenuBar::item {
    background-color: transparent;
    color: #E8EAF6;
}

/* Elementos seleccionados en la barra de menús */
QMenuBar::item:selected {
    background-color: #6200EE; /* Color de selección */
    color: #FFFFFF; /* Texto blanco cuando se selecciona */
}

/* Estilo para las acciones (QAction) */
QAction {
    font-size: 14px;
    font-weight: 600;
    color: #E8EAF6; /* Texto claro */
    background-color: transparent;
}

/* Acción en hover */
QAction:hover {
    background-color: #6200EE; /* Fondo al pasar el mouse */
    color: #FFFFFF; /* Texto blanco */
}

/* Acción presionada */
QAction:pressed {
    background-color: #3700B3; /* Fondo más oscuro al presionar */
    color: #FFFFFF; /* Texto blanco */
}

/* Botones (QPushButton) en modo oscuro */
QPushButton {
    background-color: #6200EE; /* Fondo de botón */
    color: #FFFFFF; /* Texto claro */
    border-radius: 8px;
    font-size: 14px;
    font-weight: bold;
    padding: 12px 20px;
}

/* Hover y pressed para botones */
QPushButton:hover {
    background-color: #3700B3;
}

QPushButton:pressed {
    background-color: #3100A0;
}

/* Campos de texto (QLineEdit) en modo oscuro */
QLineEdit {
    border: 2px solid #6200EE;
    border-radius: 8px;
    background-color: #333333; /* Fondo oscuro */
    color: #E8EAF6; /* Texto claro */
    font-size: 14px;
    padding: 10px 14px;
}

QLineEdit:focus {
    border-color: #03DAC5; /* Resaltar el borde al enfocar */
}

/* Otros componentes como labels */
QLabel {
    color: #E8EAF6; /* Texto claro */
    font-size: 14px;
    font-weight: 600;
}

